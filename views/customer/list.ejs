<!DOCTYPE html>
<html>
<head>
<title><%= title %></title>
</head>
<body>
<h1><%= title %></h1>
<p>Welcome to Customer list <%= name %></p>
<a href="/">Home</a>
<a href="/logout">logout</a><br>
    <br>
    <br>
    <table id="rev_list">
        <tr>
            <th>ResvID</th>
            <th>Email</th>
            <th>RoomNumber</th>
            <th>CheckinDate</th>
            <th>CheckoutDate</th>
            <th>Is_Customer</th>
            <th>ResvState</th>
            <th>Price</th>
            <th>Bank</th>
            <th>Account</th>
            <th>RoomPrice</th>
            <th>RoomType</th>
        </tr>
    </table>

    <script>
        var table = document.getElementById("rev_list");
        var data = <%- data %>;

        var row;
        var cell0, cell1, cell2, cell3, cell4, cell5, cell6, cell7, cell8, cell9, cell10, cell11;
        var d;
        for(var i=0; i<data.length; i++){
            d = data[i];
            console.log(data[i]);
            row = table.insertRow(-1);
            cell0 = row.insertCell(0);
            cell1 = row.insertCell(1);
            cell2 = row.insertCell(2);
            cell3 = row.insertCell(3);
            cell4 = row.insertCell(4);
            cell5 = row.insertCell(5);
            cell6= row.insertCell(6);
            cell7 = row.insertCell(7);
            cell8 = row.insertCell(8);
            cell9 = row.insertCell(9);
            cell10 = row.insertCell(10);
            cell11 = row.insertCell(11);
            cell0.innerHTML = d.ResvID;
            cell1.innerHTML = d.Email;
            cell2.innerHTML = d.RoomNumber;
            cell3.innerHTML = d.CheckinDate;
            cell4.innerHTML = d.CheckoutDate;
            cell7.innerHTML = d.Price;
            cell11.innerHTML = d.RoomType;
        }
    </script>
</body>
</html>
