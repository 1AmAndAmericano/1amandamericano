<script>
    function confirmresv(){
        var confirm_msg = confirm("Confirm Reservations for credited resv? ");
        if (confirm_msg == true){
                alert("Confirmation Success");
        }
    }     
    function autocancel(){
        var confirm_msg = confirm("Auto-cancel Reservations?");
        if (confirm_msg == true){
                alert("Autocancel Success");
        }
    }   
    function noshow(){
        var confirm_msg = confirm("Send E-mails for no-show Reservations?");
